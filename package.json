{
  "name": "test1",
  "version": "1.0.0",
  "description": "Proyecto de ejemplo - compila SCSS a CSS",
  "private": true,
  "scripts": {
    "build-css": "sass SRC/main.scss:ASSETS/CSS/main.css --no-source-map --style=expanded",
    "build-css-prod": "sass SRC/main.scss:ASSETS/CSS/main.css --no-source-map --style=compressed",
    "watch-css": "sass --watch SRC/main.scss:ASSETS/CSS/main.css",
    "start": "npm run watch-css"
  },
  "devDependencies": {
    "sass": "^1.80.0"
  }
}
